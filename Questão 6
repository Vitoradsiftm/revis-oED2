def bubble_rapido(lista):
    n = len(lista)
    for i in range(n):
        trocou = False
        for j in range(0, n - i - 1):
            if lista[j] > lista[j + 1]:
                lista[j], lista[j + 1] = lista[j + 1], lista[j]  # troca os elementos
                trocou = True
        if not trocou:  # se não houve troca, a lista já está ordenada
            break
    return lista
